<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>test-app</title>
    <link href='https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Material+Icons' rel="stylesheet">
    <link href="static/vendor/css/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="static/vendor/css/vuetify.min.css" rel="stylesheet">
    <% if (htmlWebpackPlugin.options.nodeModules) { %>
    <script>
        require('module').globalPaths.push('<%= htmlWebpackPlugin.options.nodeModules.replace(/\\/g, '\\\\') %>')
    </script>
    <% } %>
    <style type="text/css">
        ::-webkit-scrollbar {
            display: none;
        }

        * {
            -moz-user-select: none;
            -webkit-user-select: none;
            -ms-user-select: none;
            user-select: none;
        }

        body {
            overflow: hidden;
        }
    </style>
</head>
<body>
<div id="app"></div>
<script type="text/javascript" src="static/vendor/js/jquery.min.js"></script>
<script>
    if (process.env.NODE_ENV !== 'development') window.__static = require('path').join(__dirname, '/static').replace(/\\/g, '\\\\')
</script>
</body>
</html>
